<Window x:Class="NombramientoPartidos.View.Inicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NombramientoPartidos.View"
        xmlns:arbitroView="clr-namespace:NombramientoPartidos.View"
        mc:Ignorable="d"
        Title="Inicio" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate x:Key="EEE">
            <Ellipse>
                <Ellipse.Fill>
                    <ImageBrush ImageSource=""></ImageBrush>
                </Ellipse.Fill>
            </Ellipse>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <TabControl>
            <TabItem Header="Arbitros">
                <arbitroView:Arbitros></arbitroView:Arbitros>    
            </TabItem>
            <TabItem Header="Asignamientos">
                <StackPanel>
                    <ComboBox ItemsSource="{Binding}"></ComboBox>
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
